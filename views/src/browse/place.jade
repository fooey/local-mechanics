if place
	.place.panel.panel-default
		.panel-heading: h2= place.name
		.panel-body
			address
				div= place.address.street
				div
					a(href=place.geo.city.getLink())= place.geo.city.name
					|, 
					a(href=place.geo.state.getLink())= place.geo.state.abbr
					| 
					span= place.geo.zip
else
	p Error